{
  "name": "Q&A",
  "version": "0.01",
  "description": "Ask about any thing in the page",
  "permissions": ["<all_urls>"],
  "browser_action": {
    "default_title": "Q&A",
    "default_popup": "popout.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["d3.v7.min.js", "contentScript.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Q",
        "linux": "Alt+Q",
        "windows": "Alt+Q",
        "mac": "Alt+Q"
      }
    }
  }
}
